<Page x:Class="Dungeon.MainPages.Registration"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Dungeon.MainPages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Registration">

    <Grid>
        <StackPanel>
            <Button
                Name="BtnRegister"
                Click="BtnRegister_Click"
                Content="Регистрация"/>

        </StackPanel>

        <StackPanel>

            <Button
                Name="BtnBack"
                Click="BtnBack_Click"
                Content="Назад"/>
        </StackPanel>

        <StackPanel>
            <TextBox
                Name="TxbLogin"/>
            <TextBox
                Name="TxbPassword"/>
        </StackPanel>

    </Grid>
</Page>
